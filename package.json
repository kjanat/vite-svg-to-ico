{
	"name": "vite-svg-to-ico",
	"version": "2.0.0",
	"description": "A Vite plugin that converts SVG files to ICO format during the build process.",
	"keywords": [
		"vite-plugin",
		"vite",
		"svg",
		"ico",
		"favicon",
		"converter"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kjanat/vite-svg-to-ico.git"
	},
	"license": "MIT",
	"author": "Kaj Kowalski <info@kajkowalski.nl> (https://github.com/kjanat)",
	"type": "module",
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"default": "./dist/index.mjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"bd": "tsdown",
		"build": "tsdown",
		"dev": "tsdown --watch",
		"fmt": "dprint fmt",
		"prepack": "bunx prettier -w README.md >/dev/null",
		"postpack": "git restore README.md >/dev/null",
		"prepublish": "bun bd",
		"tar": "TARBALL=$(bun pm pack --quiet | tr -d '\\n'); echo \"tar=${TARBALL}\" >> ${GITHUB_OUTPUT:-/dev/stdout}",
		"test": "bun test",
		"typecheck": "tsgo --noEmit"
	},
	"dependencies": {
		"sharp": "^0.34.5"
	},
	"devDependencies": {
		"@arethetypeswrong/core": "^0.18.2",
		"@types/bun": "latest",
		"@typescript/native-preview": "^7.0.0-dev.20260225.1",
		"dprint": "^0.52.0",
		"publint": "^0.3.17",
		"tsdown": "^0.20.3",
		"typescript": "^5.9.3",
		"unplugin-unused": "^0.5.7"
	},
	"peerDependencies": {
		"vite": "^6.0.0 || ^7.0.0"
	},
	"packageManager": "bun@1.3.9",
	"publishConfig": {
		"access": "public",
		"provenance": true,
		"registry": "https://registry.npmjs.org/",
		"tag": "latest"
	}
}
